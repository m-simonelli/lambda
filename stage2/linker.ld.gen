#define S2_LOAD_ADDR 0x9000

OUTPUT_FORMAT(binary)

SECTIONS {
    . = S2_LOAD_ADDR;
    .s2entry : {
      *(.s2entry)
    }
    .text ALIGN(4K) : {
      KEEP(*(.entry))
      *(.text*)
    }
    .rodata ALIGN(4K) : {
      *(.rodata*)
    }
    .data ALIGN(4K) : {
      *(.data*)
    }
    .bss ALIGN(4K) : {
      bss_begin = .;
      *(COMMON)
      *(.bss*)
      bss_end = .;
    }
    S2_end = .;
}